{
  "name": "screeps-haskell-starter",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "watch": "shell-scripts/hpack.sh && nodemon --watch src --watch lib -e hs --exec ahc-cabal v1-build main --builddir=ahc-cabal-build-watch",
    "check": "shell-scripts/hpack.sh && ahc-cabal v1-build main --builddir=ahc-cabal-build",
    "check:all": "shell-scripts/hpack.sh && rm -rf ahc-cabal-build-from-scratch && ahc-cabal v1-build main --builddir=ahc-cabal-build-from-scratch",
    "deploy": "shell-scripts/hpack.sh && node node-scripts/build.js && node node-scripts/upload.js --distdir=build-screeps/dist/"
  },
  "author": "Evgeniy Safonov",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "args-parser": "^1.1.0",
    "moment": "^2.27.0",
    "node-fetch": "^2.6.0",
    "nodemon": "^2.0.4",
    "rollup": "^2.18.1",
    "rollup-plugin-terser": "^6.1.0"
  },
  "engines": {
    "node": ">=14.4.0",
    "npm": ">=6.14.5"
  },
  "dependencies": {
    "fast-text-encoding": "^1.0.3",
    "promise-polyfill": "^8.1.3"
  }
}
